<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.keyuan.mapper.GoodTypeMapper" >

    <select id="searchAll" resultType="GoodType">
        select
            type_id as typeId,
            type_name as typeName
        from good_type
    </select>

</mapper>